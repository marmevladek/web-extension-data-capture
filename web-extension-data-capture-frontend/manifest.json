{
    "manifest_version": 3,
    "name": "Spring Backend Extension",
    "version": "1.0",
    "description": "Chrome extension with Java Spring backend",
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "storage",
      "activeTab",
      "scripting"
    ]
  }
  